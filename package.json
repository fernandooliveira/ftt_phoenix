{
  "name": "familytreetop",
  "engines": {
    "node": ">=5.0"
  },
  "version": "3.0.1",
  "description": "familytreetop",
  "scripts": {
    "lint": "eslint -c .eslintrc --ext .js --ext .jsx app",
    "lint-prod": "eslint -c .eslintrc.prod --ext .js --ext .jsx app",
    "dev": "npm run lint && concurrently --kill-others \"npm run start-api\" \"npm run start-dev\"",
    "prod": "npm run lint-prod && npm run build && concurrently --kill-others \"npm run start-api\" \"npm run start-prod\"",
    "start-dev": "better-npm-run dev",
    "start-prod": "better-npm-run prod",
    "start-api": "better-npm-run api",
    "build": "better-npm-run build",
    "test": "better-npm-run test",
    "test-watch": "better-npm-run test-watch"
  },
  "betterScripts": {
    "dev": {
      "command": "node bin/server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "prod": {
      "command": "node bin/server.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "api": {
      "command": "nodemon --watch api bin/api.js",
      "evn": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "webpack --progress --verbose --display-error-details --config webpack/common.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "test": {
      "command": "mocha --require ./bin/test.js 'app/**/*spec.js'",
      "env": {
        "NODE_PATH": "./app",
        "NODE_ENV": "production"
      }
    },
    "test-watch": {
      "command": "mocha --require ./bin/test.js 'app/**/*spec.js' --watch",
      "env": {
        "NODE_PATH": "./app",
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
