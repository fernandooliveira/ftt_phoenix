{
  "name": "familytreetop-pheonix",
  "version": "0.0.0",
  "description": "family geneology application",
  "keywords": [
    "react",
    "webpack"
  ],
  "main": "dist/server/server.js",
  "scripts": {
    "start": "node node_modules/pm2/bin/pm2 start ./dist/server/server.js --name=app_production",
    "build-server": "node node_modules/webpack/bin/webpack --config configs/webpack.server.js",
    "build-client": "node node_modules/webpack/bin/webpack --config configs/webpack.client.js",
    "build": "node node_modules/concurrently/src/main \"npm run build-server\" \"npm run build-client\"",
    "watch-server": "node node_modules/webpack/bin/webpack --watch --verbose --config configs/webpack.server-watch.js",
    "watch-server-start": "node node_modules/just-wait --pattern \"dist/server/*.js\" && npm run start",
    "watch-client": "node node_modules/webpack-dev-server/bin/webpack-dev-server --config configs/webpack.client-watch.js",
    "watch": "node node_modules/concurrently/src/main --kill-others \"npm run watch-server-start\" \"npm run watch-server\" \"npm run watch-client\""
  },
  "author": "fernando.oliveira",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.5.1",
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.0",
    "clean-webpack-plugin": "^0.1.8",
    "concurrently": "^1.0.0",
    "express": "^4.13.4",
    "just-wait": "^1.0.3",
    "pm2": "^1.0.1",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "express": "^4.13.4",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "redux": "^3.3.1"
  },
  "engines": {
    "node": ">=0.10.32"
  }
}
